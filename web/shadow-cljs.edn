{:lein         true
 :source-paths ["src"]
 :builds       {:app {:target     :browser
                      :output-dir "resources/public/js"
                      :asset-path "/js"
                      :modules    {:app {:entries [web.core]}}
                      :devtools   {:after-load web.core/mount-root}}}
 :dev-http     {3000 {:root    "resources/public"
                      :handler web.handler/app}}}
